    "version": "2.0.0",
    "tasks": [
        {
            "label": "make demo_socket STM32F767 OS less",
            "type": "shell",
            "command": "make",
            "args": [
                "-C",
                "${workspaceFolder}/build/apps/demo_socket",
                "all+",
                "TARGET=arm-gcc-os-less-stm32f767"
            ],
            "group": "build",
            "problemMatcher": {
                "owner": "cpp",
                "fileLocation": [
                    "absolute"
                ],
                "pattern": {
                    "regexp": "^(.*):([0-9]+):([0-9]+): (warning|error): (.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            }
        },
        {
